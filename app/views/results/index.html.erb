<h1>Listing results</h1>

<table>
  <thead>
    <tr>
      <th>Grade</th>
      <th>Aps</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td><%= result.grade %></td>
        <td><%= result.aps %></td>
        <td><%= link_to 'Show', result %></td>
        <td><%= link_to 'Edit', edit_result_path(result) %></td>
        <td><%= link_to 'Destroy', result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<div style="margin-top:40px;">
<H5>Import your data</H5>
<br />
Select Results File
</div>
<div style="margin-top:50px">
<%= form_tag import_results_path, multipart: true do %>
    <div class="fileupload fileupload-new" data-provides="fileupload">
  <div class="input-append">
    <div class="uneditable-input span3">
	<i class="icon-file fileupload-exists"></i> 
	<span class="fileupload-preview"></span>
	</div><span class="btn btn-file">
	<span class="fileupload-new">Select file</span>
	<span class="fileupload-exists">Change</span>
	<%= file_field_tag :file %>
	</span>
	<%= submit_tag "Import", class: "btn fileupload-exists btn-danger" %>
<%end%>
	</div>
</div>
</div>


<%= link_to 'New Result', new_result_path %>
